  "scripts": {
    "build": "react-scripts build",
    "cmt": "git-cz",
    "eject": "react-scripts eject",
    "format": "prettier --single-quote --no-semi --trailing-comma none --tab-width=2 --write \"js/**/*.{js,jsx}\"",
