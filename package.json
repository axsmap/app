    "lint-staged": "7.2.0",
    "prettier": "1.13.7",
    "storybook-readme": "^5.0.8",
    "wix-storybook-utils": "3.0.7"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --no-semi --trailing-comma none --tab-width=2 --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
